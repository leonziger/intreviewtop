{{> main-header }}
<div class="main-content dashboard__content search-candidates-list candidate-search-offer">
    <div class="container">
        <section class="search-candidate">
            <h1 class="search-candidate__title">Пошук вакансії</h1>
            <div class="dashboard dashboard__two-columns">
                <div class="inner-container__big dashboard__main">
                    <div class="applicants__search-form">
                        <div class="fraimed-container dashboard__container">
                            <form action="" class="search-form">
                                <div class="search-form__find">
                                    <div class="search-form__search-wrap">
                                        <input name="prof" type="search" placeholder="Вкажіть ключові слова для пошуку" class="search-form__search">
                                    </div>
                                    <button class="the-button search-form__button">Шукати</button>
                                </div>
                                <div class="search-form__selectors">
                                    <div class="search-form__selectors-left">
                                        Шукати у
                                        <select name="where" class="base__subtitle search-form__select">
                                            <option value="1">назвi в резюме</option>
                                            <option value="2">вибiр 2</option>
                                            <option value="3">якийся довгий селектор, що не влазить в екран</option>
                                        </select>
                                    </div>
                                    <div class="search-form__selectors-right">
                                        за
                                        <select name="period" class="base__subtitle search-form__select">
                                            <option value="1">3 місяці</option>
                                            <option value="2">6 місяців</option>
                                            <option value="3">якийся довгий селектор, що не влазить в екран</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="search-form__tags-subscribe">
                                    <div class="search-form__tags nowrap">
                                        <div class="base__notes search-form__tag">Київ</div>
                                        <div class="base__notes search-form__tag">Python</div>
                                        <div class="base__notes search-form__tag">1000-2200$</div>
                                        <div class="base__notes search-form__tag">Upper-intermediate</div>
                                        <div class="base__notes search-form__tag">Intermediate</div>
                                    </div>
                                    <div class="search-form__subscribe search-form__subscribe_disable"></div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="applicants__search-results">
                        <div class="applicants__search-description">Знайдено <span class="bold">22</span> резюме</div>
                        <div class="applicants__search-control applicants__search-control_disable">
                            Показувати
                        </div>
                    </div>
                    <div class="vacancies__list">
                        <div class="vacancy">
                            <div class="fraimed-container dashboard__container">
                                <div class="candidate__first-line vacancy__line-first">
                                    <a href="#" class="base__title candidate__name">Senior Backend Developer</a>
                                    <div class="candidate__date vacancy__date">12 Годин тому</div>
                                </div>
                                <div class="vacancy__line-second">
                                    <div class="vacancy__salary-location">
                                        <div class="vacancy__location">Київ</div>
                                    </div>
                                    <div class="vacancy__salary vacancy__salary_right blue">2000-2500$</div>
                                </div>
                                <div class="vacancy__line-third">
                                    <div class="vacancy__experience">
                                        <div class="candidate__item-line">
                                            <div class="candidate__item-name">Досвід роботи:</div>
                                            <div class="candidate__item-value">3 роки</div>
                                        </div>
                                        <div class="candidate__item-line">
                                            <div class="candidate__item-name">Вид зайнятості:</div>
                                            <div class="candidate__item-value">Remote</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="vacancy__description">We are looking for a Backend Developer who is motivated to combine the art of design with the art of programming...</div>
                                <ul class="vacancy__tags">
                                    <li class="vacancy__tag">Python</li>
                                    <li class="vacancy__tag">Django</li>
                                    <li class="vacancy__tag">DRF</li>
                                </ul>
                                <div class="vacancy__contact">
                                    <a href="#" class="vacancy__recruiter">Alexander Ruban</a>, <span class="vacancy__position">Ceo</span> в <span class="vacancy__company">Interview.top</span>
                                </div>
                            </div>
                        </div>
                        <div class="vacancy">
